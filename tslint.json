{
  "extends": [
      "tslint-angular"
  ],
  "rules": {
      "directive-selector": [
          true,
          "attribute",
          [
              "brickchain",
              "integrity"
          ],
          "camelCase"
      ],
      "component-selector": [
          true,
          "element",
          [
              "brickchain",
              "integrity"
          ],
          "kebab-case"
      ],
      "no-non-null-assertion": false
  }
}
